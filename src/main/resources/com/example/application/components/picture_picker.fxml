<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="question_vbox" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.application.frontend.components.PicturePickerController">
    <children>
      <HBox>
         <children>
                <Button mnemonicParsing="false" onAction="#onActionUploadPicture" styleClass="btn_dark" text="%upload_picture">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></Button>
                <Button mnemonicParsing="false" onAction="#onAddLatexBtnClick" styleClass="btn_dark" text="%add_latex">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
         </children>
      </HBox>
        <HBox fx:id="displayImages" spacing="10.0" />
    </children>
</VBox>
