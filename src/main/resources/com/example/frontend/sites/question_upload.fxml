<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="630.0" prefWidth="1120.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.frontend.controller.QuestionUpload_ScreenController">
   <children>
      <HBox maxHeight="100.0" maxWidth="1120.0" minHeight="100.0" minWidth="1120.0" prefHeight="100.0" prefWidth="1120.0" styleClass="main_navigation" stylesheets="@../css/main.css">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/FH_logo.png" />
               </image></ImageView>
            <Label alignment="CENTER" prefHeight="129.0" prefWidth="1151.0" styleClass="homeScreen_label" text="Question Upload" textFill="WHITE">
               <font>
                  <Font size="35.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="530.0" prefWidth="1120.0">
         <children>
            <GridPane maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" styleClass="main_navigation" stylesheets="@../css/main.css">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <VBox fx:id="nav_createTestAutomaticButton" alignment="CENTER" onMouseClicked="#onCreateAutTestNavBtnClick" prefHeight="200.0" prefWidth="100.0" stylesheets="@../css/main.css">
                     <children>
                        <Button alignment="BOTTOM_LEFT" styleClass="menu_btn">
                           <graphic>
                              <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/file_add.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="92.0" styleClass="navigation_item_label_notSelected" stylesheets="@../css/main.css" text="Create Test&#10;automatic" textAlignment="CENTER" textFill="#f2eaea" />
                     </children>
                  </VBox>
                  <VBox fx:id="nav_createTestManual_button" alignment="CENTER" onMouseClicked="#onCreateManTestNavBtnClick" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
                     <children>
                        <Button styleClass="menu_btn">
                           <graphic>
                              <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/file_add.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="110.0" styleClass="navigation_item_label_notSelected" stylesheets="@../css/main.css" text="Create Test&#10;manually" textAlignment="CENTER" textFill="WHITE" />
                     </children>
                  </VBox>
                  <VBox fx:id="nav_uploadQuestionButton" alignment="CENTER" onMouseClicked="#onUploadQuestionNavBtnClick" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="2">
                     <children>
                        <Button styleClass="menu_btn">
                           <graphic>
                              <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/file_upload_blue.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="122.0" styleClass="navigation_item_label_selected" stylesheets="@../css/main.css" text="Upload&#10;Question" textAlignment="CENTER" textFill="WHITE" />
                     </children>
                  </VBox>
                  <VBox fx:id="nav_editQuestionButton" alignment="CENTER" onMouseClicked="#onEditQuestionNavBtnClick" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="3">
                     <children>
                        <Button fx:id="upload_btn1" styleClass="menu_btn">
                           <graphic>
                              <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../icons/file_edit_white.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="122.0" styleClass="navigation_item_label_notSelected" stylesheets="@../css/main.css" text="Edit&#10;Question" textAlignment="CENTER" textFill="WHITE" />
                     </children>
                  </VBox>
               </children>
            </GridPane>
            <ScrollPane fx:id="scrollPane" prefHeight="600.0" prefWidth="1042.0" stylesheets="@../css/main.css">
               <content>
                  <VBox prefHeight="720.0" prefWidth="1000.0" styleClass="automatic_create_scrollPane" stylesheets="@../css/main.css">
                     <children>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label fx:id="topicLabel" prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Topic" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox prefHeight="33.0" prefWidth="1000.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <MenuButton fx:id="category" mnemonicParsing="false" styleClass="automatic_create_dropdown" stylesheets="@../css/main.css" text="Choose category...">
                                    </MenuButton>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Difficulty" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox prefHeight="33.0" prefWidth="1000.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <Slider fx:id="difficulty" blockIncrement="1.0" majorTickUnit="2.0" max="10.0" min="1.0" minorTickCount="1" showTickLabels="true" showTickMarks="true" snapToTicks="true" style="-fx-background-color: #2f2f2f;" styleClass="slider-tool" value="5.0" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Points" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox prefHeight="33.0" prefWidth="1000.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <Spinner fx:id="points" styleClass="automatic_create_spinner" stylesheets="@../css/main.css" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Multiple Choice" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox fx:id="multipleChoiceVBox" prefHeight="33.0" prefWidth="1000.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <CheckBox fx:id="multipleChoice" mnemonicParsing="false" onAction="#onActionMultipleChoice" text="CheckBox" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label fx:id="languageLabel" prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Language" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox prefHeight="33.0" prefWidth="1000.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <MenuButton fx:id="language" mnemonicParsing="false" styleClass="automatic_create_dropdown" stylesheets="@../css/main.css" text="Choose language...">
                                    </MenuButton>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Question Text" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox minHeight="200.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <TextArea fx:id="question" prefWidth="500.0" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Label prefWidth="1000.0" styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Remarks" textFill="#e8e4e4">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <VBox minHeight="200.0" prefWidth="1000.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <TextArea fx:id="remarks" prefWidth="1000.0" />
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Label styleClass="automatic_create_label" stylesheets="@../css/main.css" text="Keywords" />
                              <VBox fx:id="keywordVBox" prefHeight="200.0" prefWidth="100.0" styleClass="upload_question_vbox" stylesheets="@../css/main.css">
                                 <children>
                                    <HBox fx:id="keywordsHBox" prefHeight="100.0" prefWidth="200.0" />
                                    <MenuButton fx:id="keyword" mnemonicParsing="false" text="Choose keyword...">
                                    </MenuButton>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="150.0" prefWidth="1000.0">
                           <children>
                              <Button fx:id="upload" maxWidth="270.0" mnemonicParsing="false" onAction="#onActionUpload" prefHeight="40.0" prefWidth="270.0" styleClass="automatic_create_uploadButton" stylesheets="@../css/main.css" text="Upload" textFill="WHITE">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Button>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </HBox>
   </children>
</VBox>
